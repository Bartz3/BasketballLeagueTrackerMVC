@using Azure.Core;
@model List<Player>
@{
    ViewData["Title"] = "Dostępni zawodnicy";
    var teamName = @TempData["TeamName"];
}


@*<partial name="_Notification" />*@
<div class="container">
    <div class="row pt-3">
        <div class="col-6">
            <h2 class="text-primary">
                Dodaj zawodnika do @teamName
            </h2>
        </div>
        <div class="col-6 text-end">
        </div>
    </div>
    <table id="playerData" class="table table-bordered table-striped" style="width:100%" data-from-addplayertoteam="true" data-from-team-name=@teamName data-team-id="@ViewBag.TeamId">
        <thead>
            <tr>
                <th></th>
                <th>Zdjęcie</th>
                <th>Imie</th>
                <th>Nazwisko</th>
                <th>Pozycja</th>
                <th>Drużyna</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts{

    <script src="~/js/player.js"></script>
}
